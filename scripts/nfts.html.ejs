<style>
  .nft {
    width: 100px;
    float: left;
    margin: 5px;
  }
  .nft img {
    width: 100%;
  }
  .nft-color {
    background: #000;
    color: #FFF;
    text-align: center;
    padding: 5px;
  }
</style>
<% nfts.map(nft => { %>
  <div class="nft">
    <img src="https://ipfs.infura.io/ipfs/<%= nft.path %>" />
    <div class="nft-color">
      #<%= nft.hex.toUpperCase() %>
    </div>
  </div>
<% }) %>
