<style>
  .nft {
    float: left;
    margin: 5px;
    border: 3px solid #000;
  }
  .nft-image {
    background: #EEE;
    width: 100px;
    height: 100px;
  }
  .nft-image img {
    width: 100%;
  }
  .nft-caption {
    background: #000;
    color: #FFF;
    text-align: center;
    padding: 5px;
  }
</style>
<% nfts.map(nft => { %>
  <div class="nft">
    <div class="nft-image">
      <img src="<%= nft.image %>" />
    </div>
    <div class="nft-caption">
      #<%= nft.hex.toUpperCase() %>
    </div>
  </div>
<% }) %>
