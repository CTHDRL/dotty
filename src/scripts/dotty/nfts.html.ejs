<style>
  .nft {
    float: left;
    margin: 5px;
    border: 3px solid #000;
  }
  .nft-image {
    background: #EEEEEE;
    width: 180px;
    height: 180px;
  }
  .nft-image img {
    width: 100%;
  }
  .nft-caption {
    background: #000000;
    color: #FFFFFF;
    padding: 5px;
    font-family: Arial;
    font-size: 10px;
    line-height: 1.6;
  }
  .nft-caption a {
    color: #FFFFFF;
  }
</style>
<% nfts.map(nft => { %>
  <div class="nft">
    <div class="nft-image">
      <img src="<%= nft.metadata.image %>" />
    </div>
    <div class="nft-caption">
      <div class="nft-caption-item">
        NAME: <%= nft.metadata.name %>
      </div>
      <div class="nft-caption-item">
        DESCRIPTION: <%= nft.metadata.description %>
      </div>
      <div class="nft-caption-item">
        CREATED: <%= moment(nft.metadata.date, 'X').format('MM/DD/YY') %>
      </div>
      <% nft.metadata.attributes.map(attribute => { %>
        <div class="nft-caption-item">
          <%= attribute.trait_type.toUpperCase() %>: <%= attribute.value %>
        </div>
      <% }) %>
      <div class="nft-caption-item">
        <a href="<%= nft.metadataUrl %>">view metadata</a>
      </div>
    </div>
  </div>
<% }) %>
